{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "cypress/globals": true
    },
    "extends": [
        "airbnb-base"
    ],
    "plugins": [
      "cypress"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "indent": [ 
        "error", 
        2
      ],
      "semi": [
        "error", 
        "never"
      ]
    }
}
